<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="citruscircuits.scout">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name="._superDataClasses.AppCc"
        android:allowBackup="true"
        android:icon="@drawable/newscoutlogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:screenOrientation="landscape"
        android:largeHeap="true">
        <activity
            android:name=".A0A"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".A0B"
            android:screenOrientation="landscape" />

        <activity android:name=".A1A"
            android:label="Map"
            android:screenOrientation="landscape" />

        <activity android:name=".A2A"
            android:screenOrientation="landscape" />

        <activity android:name=".A3A"
            android:screenOrientation="landscape" />

        <activity android:name=".utils.QRScan" />

    </application>
</manifest>
